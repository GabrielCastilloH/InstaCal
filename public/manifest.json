{
    "manifest_version": 3,
    "name": "InstaCal",
    "version": "1.0.0",
    "description": "Add events to Google Calendar with natural language.",
    "action": {
        "default_popup": "index.html",
        "default_title": "InstaCal"
    },
    "permissions": [
        "identity",
        "storage"
    ],
    "host_permissions": [
        "https://www.googleapis.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}